          <h2>Users</h2>
            <table class="table table-hover table-inverse">
              <thead>
                <tr>
                  <th></th>
                  <th>image</th>
                  <th>name</th>
                  <th colspan-"3"></th>
                </tr>
              </thead>
              <tbody>
                <%= @books.each do |book| %>
                <%= image_tag @book.get_image %>
                <tr>
                  <td><%= image_tag @user.get_profile_image(100,100) %></td>
                  <td><%= @user.name %></td>
                  <td><%= link_to "show", user_path(@user.id) %></td>
                </tr>
                <% end %>
              </tbody>
            </table>